{
  "name": "@hansolo/cli",
  "version": "1.0.0",
  "description": "Git workflow automation tool that enforces linear history and prevents merge conflicts",
  "main": "dist/index.js",
  "bin": {
    "hansolo": "./bin/hansolo.js",
    "hansolo-mcp": "./bin/hansolo-mcp"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "package": "npm run build && npm pack"
  },
  "keywords": [
    "git",
    "workflow",
    "automation",
    "mcp",
    "linear-history",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/hansolo.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "bin",
    "templates",
    "scripts"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@gitbeaker/node": "^35.8.0",
    "@octokit/rest": "^20.0.0",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "ora": "^7.0.1",
    "simple-git": "^3.20.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "playwright": "^1.40.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}